<?xml version="1.0" encoding="UTF-8"?>
<wls:weblogic-web-app xmlns:wls="http://www.bea.com/ns/weblogic/weblogic-web-app"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 	http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd http://www.bea.com/ns/weblogic/weblogic-web-app http://www.bea.com/ns/weblogic/weblogic-web-app/1.2/weblogic-web-app.xsd">
	<wls:weblogic-version>10.3.3</wls:weblogic-version>
	<wls:context-root>/alipaybank</wls:context-root>
	<wls:container-descriptor>
		<prefer-application-packages>  
		   <package-name>org.apache.commons.lang.*</package-name>
 	            <package-name>antlr.*</package-name>
	            <package-name>org.hibernate.*</package-name>
	            <package-name>javax.persistence.*</package-name>
        </prefer-application-packages>
	</wls:container-descriptor>
</wls:weblogic-web-app>