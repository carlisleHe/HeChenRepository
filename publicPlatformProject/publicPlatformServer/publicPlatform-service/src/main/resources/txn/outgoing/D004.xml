<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TxnDefines xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="TxnDefines.xsd">
	<TxnDefine id="D004" target="tuxedo">
		<annotation>兴业宝账户交易明细查询</annotation>
		<Request name="MAIN">
			<ItemInfo name="header" ref="Z02RequestHeader" descript="Z02请求头"/>
			<ItemInfo name="JYDM1" length="4" descript="内部交易代码" default="D004"/>
		    <ItemInfo name="ZJLX" length="1"  descript="用户证件类型"/>
			<ItemInfo name="ZJHM" length="32" descript="用户证件号码"/>
			<ItemInfo name="ZHDH" length="18" descript="银行卡号"/>
		    <ItemInfo name="QSRQ" type="DATE" descript="起始日期"/>
		    <ItemInfo name="JSRQ" type="DATE" descript="结束日期"/>
		    <ItemInfo name="CXYH" type="NUMBER_08_0" descript="查询页号"/>
		    <ItemInfo name="CXJLS" type="NUMBER_02_0" descript="查询记录数"/>
		</Request>
		<Response name="MAIN">
			<ItemInfo name="header" ref="Z02ResponseHeader" descript="Z02响应头"/>
			<ItemInfo name="ZHDH" length="18" descript="银行卡号"/>
		    <ItemInfo name="ZJLS" type="NUMBER_08_0" descript="总记录数"/>
		    <CompositeItem name="JYMXX" infoName="CFCS">
		       <ItemInfo name="DDLX" length="1"  descript="订单类型"/>
		       <ItemInfo name="YHDDH" length="32"  descript="银行订单号"/>
		       <ItemInfo name="JJGSDDH" length="32"  descript="基金公司订单号"/>
		       <ItemInfo name="DDZT" length="1"  descript="订单状态"/>
		       <ItemInfo name="JYFE" type="NUMBER_15_2"  descript="交易份额"/>
		       <ItemInfo name="WCSJ" type="DATETIME"  descript="交易完成时间"/>
		       <ItemInfo name="JE" type="NUMBER_15_2"  descript="交易金额"/>
		    </CompositeItem>
		</Response>
		<ExceptResp name="MAIN">
	       <ItemInfo name="header" ref="Z02ResponseHeader" descript="Z02异常头"/>
   		</ExceptResp>
	</TxnDefine>
</TxnDefines>