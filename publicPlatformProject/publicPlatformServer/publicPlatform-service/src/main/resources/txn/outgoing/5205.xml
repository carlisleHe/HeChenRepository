<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<TxnDefines xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="TxnDefines.xsd">
	<TxnDefine id="5205" target="tuxedo" isFixTxnHeader="false">
		<annotation>客户理财产品信息查询</annotation>
		<Request name="MAIN">
			<ItemInfo name="header" ref="FinancialProductRequestHeader"/>
			<ItemInfo length="1" name="JYZL" /><!-- 交易种类 -->
			<ItemInfo length="1" name="DYBZ" default="1"/><!-- 打印标志 -->
			<ItemInfo length="1" name="FWFS" default="2"/><!-- 服务方式 1-对客户    2-对柜员 -->
			<ItemInfo length="18" name="ZHDH" /><!--账户代号 -->
			<ItemInfo name="JYC" length="7" default="0000000"/> <!--校验串-->
			<ItemInfo length="9" name="PZDH" /><!-- 凭证代号-->
			<ItemInfo length="6" name="MM" /><!-- 密码-->
			<ItemInfo length="8" name="CPDM" /><!--产品代码 -->
			<ItemInfo type="DATE" name="QSRQ" /><!-- 起始日期 -->
			<ItemInfo type="DATE" name="ZZRQ" /><!--终止日期 -->
		</Request>
		<Response name="MAIN">
			<ItemInfo name="header" ref="FinancialProductResponseHeader"/>	
			<ItemInfo length="30" name="INFILE"  /> <!-- 文件名 -->
			<ItemInfo length="8" name="WJJYM"  /> <!-- 文件校验码 -->
		</Response>
		<ExceptResp name="MAIN">
			<ItemInfo name="TUXHEAD" ref="TuxedoResponseHeader" descript="TUX头"/>
			 <ItemInfo name="MSGID" type="NUMBER_05_0"/><!-- 信息标识 -->
		</ExceptResp>
	</TxnDefine>
</TxnDefines>

