<?xml version="1.0" encoding="UTF-8"?>

<TxnDefines>
	<TxnDefine id="2821" target="tuxedo" isFixTxnHeader="true">
		<annotation>客户签约管理</annotation>
		<Request name="MAIN">
			<ItemInfo name="header" ref="TuxedoRequestHeader"/>
