		    <ItemInfo name="JYSJ" length="6" descript="交易时间"/>
		    <ItemInfo name="GYLSH" length="8" descript="柜员流水号"/>-->
		    <ItemInfo length="60" name="BZ" descript="备注"/>
			<ItemInfo length="1" name="JLZT" descript="记录状态"/>
			<ItemInfo length="50" name="WMJ" descript="文件名"/>
		</Response>
		<ExceptResp name="MAIN">
			<ItemInfo name="header" ref="TuxedoResponseHeader" descript="TUX头"/>
		</ExceptResp>
	</TxnDefine>
</TxnDefines>
