<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"../struts-2.0.dtd">
<struts>
	<constant name="struts.objectFactory"
		value="org.apache.struts2.spring.StrutsSpringObjectFactory" />
	<constant name="struts.convention.action.fileProtocols" value="jar,zip" />
	<constant name="struts.convention.action.includeJars" value=".*?/_wl_cls_gen.*jar(!/)?" />
	<constant name="struts.convention.action.suffix" value="Action" />
	<constant name="struts.convention.action.mapAllMatches" value="true" />
	<constant name="struts.convention.result.path" value="/secure" />
	<constant name="struts.convention.action.disableJarScanning"
		value="false" />
	<constant name="struts.ui.theme" value="jqueryui" />
	<constant name="struts.ui.templateDir" value="Template" />
	<constant name="struts.ui.templateSuffix" value="ftl" />
	<constant name="struts.i18n.encoding" value="utf-8"></constant>
	<constant name="struts.devMode" value="false"></constant>
	<constant name="struts.action.excludePattern" value="/AddInImage"></constant>
	<package name="alipay-default" extends="struts-default">
		<result-types>
			<result-type name="GlobalJsonErrorResult"
				class="com.cib.alipayserver.web.result.GlobalJsonErrorResult" />
			<result-type name="json" class="org.apache.struts2.json.JSONResult" />
		</result-types>
		<interceptors>
			<interceptor name="bean-scope" class="com.intensoft.scopeplugin.ScopeInterceptor" />
			<interceptor name="securityInterceptor" class="com.cib.alipayserver.web.interceptor.SecurityInterceptor"></interceptor>
			<interceptor name="myException" class="com.cib.alipayserver.web.interceptor.ExceptionInterceptor" />
			<interceptor-stack name="base_stack">
				<interceptor-ref name="exception" />
				<interceptor-ref name="servletConfig" />
				<interceptor-ref name="prepare" />
				<interceptor-ref name="checkbox" />
				<interceptor-ref name="multiselect" />
				<interceptor-ref name="actionMappingParams" />
				<interceptor-ref name="conversionError" />
				<interceptor-ref name="bean-scope" />
				<interceptor-ref name="myException" />
				<interceptor-ref name="securityInterceptor" />
				<interceptor-ref name="params">
					<param name="excludeParams">dojo\..*,^struts\..*,^session\..*,^request\..*,^application\..*,^servlet(Request|Response)\..*,parameters\...*,^class\..*</param>
				</interceptor-ref>
			</interceptor-stack>
			<interceptor-stack name="clean_stack">
				<interceptor-ref name="exception" />
				<interceptor-ref name="servletConfig" />
				<interceptor-ref name="prepare" />
				<interceptor-ref name="checkbox" />
				<interceptor-ref name="multiselect" />
				<interceptor-ref name="actionMappingParams" />
				<interceptor-ref name="conversionError" />
				<interceptor-ref name="bean-scope" />
				<interceptor-ref name="myException" />
				<interceptor-ref name="params">
					<param name="excludeParams">dojo\..*,^struts\..*,^session\..*,^request\..*,^application\..*,^servlet(Request|Response)\..*,parameters\...*,^class\..*</param>
				</interceptor-ref>
			</interceptor-stack>
		</interceptors>

		<default-interceptor-ref name="base_stack" />
		<global-results>
			<result name="globalError">/error_mobile.jsp</result>
			<result name="jsonError" type="GlobalJsonErrorResult">
			</result>
		</global-results>
	</package>
</struts>